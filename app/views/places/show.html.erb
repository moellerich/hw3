<h1><%= @place["name"] %></h1>

<p>
  <a href="/entries/new?place_id=<%= @place.id %>">New entry for <%= @place.name %></a>
</p>
<p>



</p>
<p>
  <% for entry in @entries %>
    <b>
      <%= entry["title"] %>
    </b>
    <p>
      <%= entry["posted_on"] %>
    </p>
    <p>
      <%= entry["description"] %>
    </p>
  <% end %>
</p>


<a href="/places">List of All Places I've Been</a>

<p>
  <%= button_to "Delete this place", @place, :method => "delete" %>
</p>